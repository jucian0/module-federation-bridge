{
  "name": "@module-federation-bridge/react",
  "version": "0.0.0",
  "repository": {
    "url": "https://github.com/jucian0/module-federation-react-bridge"
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup src/index.ts --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "react-router-dom": "^6.2.1",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "tsup": "^6.7.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "dts": true,
    "splitting": true,
    "sourcemap": true,
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react-dom",
      "react",
      "react-router-dom"
    ]
  }
}